{
  "kind": "collectionType",
  "collectionName": "product_dummies",
  "info": {
    "singularName": "product-dummy",
    "pluralName": "product-dummies",
    "displayName": "productDummy",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "required": true
    },
    "description": {
      "type": "text"
    },
    "image": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": true,
      "required": true
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "tshirt",
        "mug",
        "hoodie",
        "stickers"
      ],
      "default": "tshirt",
      "required": true
    },
    "size": {
      "type": "string"
    },
    "color": {
      "type": "enumeration",
      "enum": [
        "red",
        "blue",
        "green",
        "yellow",
        "white"
      ],
      "required": true
    },
    "price": {
      "type": "integer",
      "required": true
    },
    "availableQty": {
      "required": true,
      "type": "integer"
    }
  }
}
